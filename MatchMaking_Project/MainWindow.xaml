        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MatchMaking_Project"
        mc:Ignorable="d"
          Title="MainWindow" Height="507" Width="800"  WindowStartupLocation="CenterScreen" Icon= "Ring.png">

